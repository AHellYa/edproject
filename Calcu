    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        Calculadora a = new Calculadora(jLabel1.getText());
        try{
        jLabel1.setText(a.resultado2());
        }catch(Exception e){
        jLabel1.setText(revSintax(a.resultado2()));
            
       }
    } 
                                            

